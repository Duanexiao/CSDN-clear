{
  "name": "CSDN-clear",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "自动查看更多",
  "icons": {
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "./images/icon128.png",
    "default_title": "interest"
  },
  "content_scripts": [{
    "matches": ["https://blog.csdn.net/*"],
    "js": ["main.js"]
  }]
}